void main(void) {
    print('a');
    print(1 < 2);
    print(3 < 2);
    print(2 < 2);

    print('b');
    print(1 > 2);
    print(3 > 2);
    print(2 > 2);

    print('c');
    print(1 <= 2);
    print(3 <= 2);
    print(2 <= 2);

    print('d');
    print(1 >= 2);
    print(3 >= 2);
    print(2 >= 2);

    print('e');
    print(1 == 2);
    print(3 == 2);
    print(2 == 2);

    print('f');
    print(1 != 2);
    print(3 != 2);
    print(2 != 2);

    print('g');
    print(1 && 0);
    print(0 && 0);
    print(1 && 1);

    print('h');
    print(1 || 0);
    print(0 || 0);
    print(1 || 1);

    print('i');
    print(!1);
    print(!0);
}